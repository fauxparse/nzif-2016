- content_for :title, t('.title')
- content_for :navigation_button, back_button(admin_packages_path(festival))

%header
  %section
    %h1= t('.title')
    %h2= festival.name

%section
  = form_for package_form, url: admin_packages_path(festival) do |form|
    = render partial: "form", object: form
    %footer
      = form.submit t('.submit')
